@if (BuildData.StatusPhase == "Complete")
{
    <img src="css/pics/check-circle.svg" title="@BuildData.BuildName -> @BuildData.StatusPhase" style="width:24px;height:24px;">
}
else if (BuildData.StatusPhase == "Running")
{
    <img src="css/pics/play-circle.svg" title="@BuildData.BuildName -> @BuildData.StatusPhase" style="width:24px;height:24px;">
}
else
{
    <img src="css/pics/alert-circle.svg" title="@BuildData.BuildName -> @BuildData.StatusPhase" style="width:24px;height:24px;">
}